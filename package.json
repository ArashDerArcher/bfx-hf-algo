{
  "name": "bfx-hf-algo",
  "version": "2.0.15",
  "description": "HF Algorithmic Order Module",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Anton Nazarenko <anton@code-care.pro>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test NODE_PATH=lib/ nyc --check-coverage --lines 90 --branches 60 --functions 90 --statements 90 --reporter=lcov --reporter=html mocha -b --recursive",
    "test-without-coverage": "NODE_ENV=test mocha -R spec -b --recursive",
    "macrossover_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ma_crossover/index.js > docs/ma_crossover.md",
    "aohost_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ao_host.js > docs/ao_host.md",
    "helpers_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/host/gen_helpers.js > docs/helpers.md",
    "iceberg_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/iceberg/index.js > docs/iceberg.md",
    "twap_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/twap/index.js > docs/twap.md",
    "pingpong_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ping_pong/index.js > docs/ping_pong.md",
    "ad_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/accumulate_distribute/index.js > docs/accumulate_distribute.md",
    "ococo_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ococo/index.js > docs/ococo.md",
    "docs": "npm run aohost_docs && npm run helpers_docs && npm run iceberg_docs && npm run twap_docs && npm run ad_docs && npm run macrossover_docs && npm run pingpong_docs && npm run ococo_docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-algo.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-algo/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "bfx-api-mock-srv": "^1.0.4",
    "bfx-api-node-core": "^1.1.8",
    "bfx-api-node-models": "^1.1.9",
    "bfx-api-node-plugin-managed-candles": "^1.0.0",
    "bfx-api-node-plugin-managed-ob": "^1.0.0",
    "bfx-api-node-plugin-wd": "^1.0.3",
    "bfx-api-node-util": "^1.0.2",
    "bfx-hf-indicators": "^2.0.0",
    "bfx-hf-models": "^2.1.0",
    "bfx-hf-util": "^1.0.1",
    "bitfinex-api-node": "^4.0.10",
    "bluebird": "^3.5.5",
    "debug": "^4.1.1",
    "eventemitter2": "^6.0.0",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "lowdb": "^1.0.0",
    "p-iteration": "^1.1.7",
    "wtfnode": "^0.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "bfx-hf-ext-plugin-bitfinex": "^1.0.5",
    "bfx-hf-models-adapter-lowdb": "^1.0.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "socks-proxy-agent": "^5.0.0",
    "standard": "^14.2.0",
    "webpack": "^4.42.0"
  }
}
